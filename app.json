{
  "expo": {
    "name": "devmeet",
    "slug": "devmeet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons8-internet-64.png",
    "splash": {
      "image": "./assets/person.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.marcelosanto.devmeet"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
